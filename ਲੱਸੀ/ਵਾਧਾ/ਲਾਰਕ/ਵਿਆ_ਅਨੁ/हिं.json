{
  "ਵਾਧਾ": ".lark",
  "ਸਰੋਤ_ਭਾ": "en",
  "ਨਿਯਮ": [
    {
      "ਸਰੋਤ": "start : ( _item | NL ) * \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "_item : rule \n     | token \n     | statement \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "rule : RULE priority ? \":\" expansions NL \n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "token : TOKEN priority ? \":\" expansions NL \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "priority : \".\" NUMBER \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "statement : \"%ignore\" expansions NL -> ignore \n         | \"%import\" import_args [ \"->\" TOKEN ] NL -> import \n         | \"%declare\" name + -> declare \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "import_args : name ( \".\" name ) * \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "expansions : alias ( VBAR alias ) * \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "alias : expansion [ \"->\" RULE ] \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "expansion : expr * \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "expr : atom [ OP | \"~\" NUMBER [ \"..\" NUMBER ] ] \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "atom : \"(\" expansions \")\" \n     | \"[\" expansions \"]\" -> maybe \n     | STRING \"..\" STRING -> literal_range \n     | name \n     | ( REGEXP | STRING ) -> literal \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "name : RULE \n    | TOKEN \n\n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "VBAR : NL ? \"|\" \n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "OP : /[+*][?]?|[?](?![a-z])/ \n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "RULE : /!?[_?]?[a-z][_a-z0-9]*/ \n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "TOKEN : /_?[A-Z][_A-Z0-9]*/ \n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "STRING : _STRING \"i\" ? \n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "REGEXP : /\\/(?!\\/)(\\\\\\/|\\\\\\\\|[^\\/\\n])*?\\/[imslux]*/ \n",
      "ਅਨੁ": "",
      "ਰੁਤਬਾ": "ਕਰਨੀ ਹੈ"
    },
    {
      "ਸਰੋਤ": "NL : /(\\r?\\n)+\\s*/ \n\n",
